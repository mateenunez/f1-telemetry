{
    "headers": [
      {
        "source": "/api/blob-proxy/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, immutable, max-age=31536000, s-maxage=31536000"
          },
          {
            "key": "CDN-Cache-Control",
            "value": "max-age=31536000"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          }
        ]
      },
      {
        "source": "/public/assets/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, immutable, max-age=31536000, s-maxage=31536000"
          },
          {
            "key": "CDN-Cache-Control",
            "value": "max-age=31536000"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          }
        ]
      },
      {
        "source": "/public/(images|fonts|videos)/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, immutable, max-age=31536000, s-maxage=31536000"
          },
          {
            "key": "CDN-Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      },
      {
        "source": "/public/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=2592000"
          }
        ]
      }
    ]
  }
  